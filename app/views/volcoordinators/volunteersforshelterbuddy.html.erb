<p id="notice"><%= notice %></p>

<h1>Volunteers that need to be entered into ShelterBuddy</h1>

<table border=1>

  <tr>
    <th>Firstname</th>
    <th>Lastname</th>
    <th></th>
  </tr>

<% @volunteers.each do |volunteer| %>
  <% if volunteer.status == "Enter into ShelterBuddy" %>
    <tr>
      <td><%= volunteer.firstname %></td>
      <td><%= volunteer.lastname %></td>
      <td><%= link_to "Show", :controller => "volcoordinators", :action => "review", :id => volunteer.id, :reviewtype => "volunteersforshelterbuddy" %></td>
    </tr>
  <% end %>
<% end %>
</table>

<br>

<%= link_to "Back", volcoordinators_path %> |
<%= link_to "Home", root_path %>