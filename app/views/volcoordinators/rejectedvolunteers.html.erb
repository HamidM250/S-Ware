<p id="notice"><%= notice %></p>

<h1>Rejected Applications</h1>

<table border=1>

  <tr>
    <th>Firstname</th>
    <th>Lastname</th>
    <th></th>
  </tr>

<% @volunteers.each do |volunteer| %>
  <% if volunteer.status == "Rejected" %>
    <tr>
      <td><%= volunteer.firstname %></td>
      <td><%= volunteer.lastname %></td>
      <td><%= link_to "Show", volunteer %>
    </tr>
  <% end %>
<% end %>
</table>

