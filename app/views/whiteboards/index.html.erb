<h1>Listing Notes</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Contents</th>
    <th></th>
  </tr>

<% @whiteboards.each do |whiteboard| %>
  <tr>
    <td><%= whiteboard.title %></td>
    <td><%= whiteboard.body %></td>
    <td><%= link_to 'Destroy', whiteboard, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />
